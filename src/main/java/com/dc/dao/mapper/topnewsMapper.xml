<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dc.dao.topnewsMapper">
	<resultMap id="topnews" type="com.dc.entity.topnews">
		<result column="id" property="id" jdbcType="BIGINT" />
		<result column="title" property="title" jdbcType="VARCHAR" />
		<result column="abstracts" property="abstracts" jdbcType="VARCHAR" />
		<result column="context" property="context" jdbcType="VARCHAR" />
		<result column="picture" property="picture" jdbcType="VARCHAR" />
	</resultMap>

	<select id="getAll" resultMap="topnews">
		select
		a.id, a.title, a.abstracts, a.context, a.picture
		from
		topnews a
		where 1 =1
	</select>
</mapper>